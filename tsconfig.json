{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types",
    "expo-env.d.ts",
    "app/index.js",
    "app/app.js",
    "app/scan.js",
    ".expo/types/**/*.ts"
  ]
}
